{
    "name": "go2digital/builder",
    "description": "A package to easily integrate GrapesJS into your laravel project.",
    "type": "library",
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": ">=5.6.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Go2Digital",
            "email": "hello@go2digital.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Go2Digital\\Builder\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Go2Digital\\Builder\\GrapesjsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
