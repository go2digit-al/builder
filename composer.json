{
  "name": "webkleur/builder",
  "description": "A package to easily integrate GrapesJS into your laravel project.",
  "type": "library",
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/support": ">=5.6.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Webkleur",
      "email": "hello@webkleur.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Webkleur\\Builder\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Webkleur\\Builder\\GrapesjsServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev"
}
